Private Sub ClearFields()
   For pnCtr = 0 To txtField.Count - 1
     Select Case pnCtr
      Case 1, 7
         txtField(pnCtr).Text = Format(oApp.ServerDate, "MMMM DD, YYYY")
      Case 11
         If txtField(pnCtr).Tag = "" Then
            txtField(pnCtr).Text = oApp.BranchName
            txtField(pnCtr).Tag = txtField(pnCtr).Text
         Else
            txtField(pnCtr).Text = txtField(pnCtr).Tag
         End If
      Case Else
         txtField(pnCtr).Text = Empty
         txtField(pnCtr).Tag = Empty
      End Select
   Next
   
   Label2.Caption = "UNKNOWN"

   With GridEditor1
      .Rows = 2
      .Col = 1
      .ColWidth(2) = 4900
      
      'empty row
      .TextMatrix(1, 1) = ""
      .TextMatrix(1, 2) = ""
      .TextMatrix(1, 3) = ""
      .TextMatrix(1, 4) = ""
      .TextMatrix(1, 5) = 0
   End With
   pbPrint = False
 End Sub

